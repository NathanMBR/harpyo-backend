{
  "name": "harpyo-backend",
  "version": "1.0.0",
  "description": "Backend side of Harpyo, a notes web app made with TypeScript, Express, Prisma, Zod, Jest, and more.",
  "main": "./build/index.js",
  "repository": "https://github.com/NathanMBR/harpyo-backend.git",
  "author": "Nathan Murillo <nathanmurillodeoliveira@gmail.com>",
  "license": "MIT",
  "scripts": {
    "postinstall": "husky install",
    "test": "jest --verbose --passWithNoTests --runInBand --testTimeout=10000",
    "test:unitary": "yarn test --testPathPattern=test/unitary",
    "test:integration": "yarn test --testPathPattern=test/integration",
    "lint": "eslint --fix",
    "build": "concurrently yarn:build:*",
    "build:types": "tsc -p tsconfig.build.json",
    "build:compile": "babel src --extensions .ts --out-dir build --source-maps --copy-files --no-copy-ignored",
    "start": "node --enable-source-maps .",
    "start:dev": "node -r dotenv/config --enable-source-maps .",
    "start:docker": "yarn prisma generate && yarn prisma migrate deploy && yarn build && yarn start:dev",
    "qs": "yarn build && yarn start:dev"
  },
  "dependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@prisma/client": "^4.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.2.2",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.7.7",
    "prisma": "^4.0.0",
    "typescript": "^4.7.4",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@swc/core": "^1.2.211",
    "@swc/jest": "^0.2.21",
    "@types/babel__core": "^7.1.19",
    "@types/babel__preset-env": "^7.9.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/eslint": "^8.4.5",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.3",
    "@types/nodemailer": "^6.4.4",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "dotenv": "^16.0.1",
    "eslint": "^8.19.0",
    "eslint-plugin-jest": "^26.5.3",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "lint-staged": "^13.0.3",
    "pg": "^8.7.3",
    "supertest": "^6.2.4",
    "ts-node": "^10.8.2"
  }
}
